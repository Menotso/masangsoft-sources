<?xml version="1.0" encoding="UTF-8" ?>
<XML ID="Replay">
	<Schema xmlns="MaietData" xmlns:dt="MaietDataTypes" />

<!--
   Replay 화면 구성
   
   Last Update : 24.DEC.2004
   Editor      : Lim Dong Hwan
-->


<!-- FRAME: 프레임 생성 : Replay -->
	<!--<FRAME item="Replay">
		<FRAMELOOK>NullFrameLook</FRAMELOOK>
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>-1</W>
			<H>-1</H>
		</BOUNDS>
		<VISIBLE>false</VISIBLE>
	</FRAME>
	-->
	<FRAME item="Replay">
		<FRAMELOOK>DefaultFrameLook</FRAMELOOK>
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>500</W>
			<H>295</H>
		</BOUNDS>
		<ALIGN>
			<HALIGN>center</HALIGN>
			<VALIGN>center</VALIGN>
		</ALIGN>
		<RESIZABLE>false</RESIZABLE>
		<VISIBLE>false</VISIBLE>
		<TEXT>STR:UI_REPLAY_01</TEXT> <!-- 리플레이 -->
	</FRAME>
	
	<!-- LABEL : 파일 선택 표시 라벨 -->
	<LABEL item="Label" parent="Replay">
		<FONT>FONTa9</FONT>
		<TEXTCOLOR>
			<R>205</R>
			<G>205</G>
			<B>205</B>
		</TEXTCOLOR>
		<BOUNDS>
			<X>16</X>
			<Y>33</Y>
			<W>266</W>
			<H>24</H>
		</BOUNDS>
		<TEXT>STR:UI_REPLAY_02</TEXT> <!-- ../내 문서/Gunz/Replay -->
	</LABEL>
	
	<!-- LISTBOX : 리플레이 파일 리스트 -->
	<LISTBOX item="Replay_FileList" parent="Replay">
		<BOUNDS>
			<X>16</X>
			<Y>56</Y>
			<W>466</W>
			<H>150</H>
		</BOUNDS>
		<ITEMHEIGHT>16</ITEMHEIGHT>
	</LISTBOX>
	
	<!-- TEXTAREA : 표시 -->
	<TEXTAREA item="Label" parent="Replay">
		<FONT>FONTa10</FONT>
		<TEXTCOLOR>
			<R>200</R>
			<G>200</G>
			<B>200</B>
		</TEXTCOLOR>
		<BOUNDS>
			<X>16</X>
			<Y>210</Y>
			<W>480</W>
			<H>50</H>
		</BOUNDS>
		<TEXT>STR:UI_REPLAY_03</TEXT> <!-- 버젼이 낮은 경우에 재생시 일부 차이가 있을 수 -->
		<EDITABLE>false</EDITABLE>
	</TEXTAREA>

	<!-- BUTTON : 리플레이 보기 버튼-->
	<BUTTON item="Replay_View" parent="Replay">
		<BUTTONLOOK>DefaultButtonLook</BUTTONLOOK>
		<ANCHORS>
			<LEFT>true</LEFT>
			<TOP>true</TOP>
			<RIGHT>true</RIGHT>
			<BOTTOM>true</BOTTOM>
		</ANCHORS>
		<BOUNDS>
			<X>215</X>
			<Y>250</Y>
			<W>133</W>
			<H>32</H>
		</BOUNDS>
		<DEFAULTKEY>ENTER</DEFAULTKEY>
		<TEXT>STR:UI_REPLAY_05</TEXT> <!-- 보기(&amp;V) -->
		<CONFIRMMESSAGE>STR:UI_REPLAY_06</CONFIRMMESSAGE> <!-- 선택한 리플레이를 보시겠습니까? -->
		<CONFIRMLOOK>DefaultFrameLook</CONFIRMLOOK>
	</BUTTON>

	<!-- BUTTON : 종료 버튼-->
	<BUTTON item="ReplayClose" parent="Replay">
		<BUTTONLOOK>DefaultButtonLook</BUTTONLOOK>
		<ANCHORS>
			<LEFT>true</LEFT>
			<TOP>true</TOP>
			<RIGHT>true</RIGHT>
			<BOTTOM>true</BOTTOM>
		</ANCHORS>
		<BOUNDS>
			<X>352</X>
			<Y>250</Y>
			<W>133</W>
			<H>32</H>
		</BOUNDS>
		<DEFAULTKEY>ESC</DEFAULTKEY>
		<TEXT>STR:UI_REPLAY_A01</TEXT> <!-- 닫기 -->
	</BUTTON>
	

	
<!-- 리플레이 -->
	<!-- 리플레이 확인창 -->
	<FRAME item="ReplayConfirm">
		<FRAMELOOK>DefaultFrameLook</FRAMELOOK>
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>350</W>
			<H>150</H>
		</BOUNDS>
		<ALIGN>
			<HALIGN>center</HALIGN>
			<VALIGN>center</VALIGN>
		</ALIGN>
		<VISIBLE>false</VISIBLE>
		<RESIZABLE>false</RESIZABLE>
		<TEXT>STR:UI_REPLAY_07</TEXT> <!-- 리플레이 다시 보기 -->
	</FRAME>

	<!-- Label : 표시 -->
	<LABEL item="Label" parent="ReplayConfirm">
		<FONT>FONTa10</FONT>
		<TEXTCOLOR>
			<R>255</R>
			<G>255</G>
			<B>255</B>
		</TEXTCOLOR>
		<ANCHORS>
			<LEFT>true</LEFT>
			<TOP>true</TOP>
			<RIGHT>false</RIGHT>
			<BOTTOM>false</BOTTOM>
		</ANCHORS>
		<BOUNDS>
			<X>15</X>
			<Y>30</Y>
			<W>250</W>
			<H>32</H>
		</BOUNDS>
		<TEXT>STR:UI_REPLAY_08</TEXT> <!-- 리플레이 동영상을 다시 보시겠습니까? -->
	</LABEL>
	
	<!-- Button : 리플레이 다시 보기 버튼 -->
	<BUTTON item="ReplayOkButton" parent="ReplayConfirm">
		<BOUNDS>
			<X>130</X>
			<Y>103</Y>
			<W>100</W>
			<H>32</H>
		</BOUNDS>
		<TEXT>STR:UI_REPLAY_09</TEXT> <!-- 다시 보기(&amp;R) -->
	</BUTTON>

	<!-- Button : 종료 버튼 -->
	<BUTTON item="Exit" parent="ReplayConfirm">
		<BOUNDS>
			<X>233</X>
			<Y>103</Y>
			<W>100</W>
			<H>32</H>
		</BOUNDS>
		<TEXT>STR:UI_REPLAY_10</TEXT> <!-- 종료(&amp;X) -->
	</BUTTON>

	<!-- Frame : 리플레이 타임라인 프레임 -->
	<FRAME item="ReplayTimeLine_Frame">
		<FRAMELOOK>NullFrameLook</FRAMELOOK>
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>300</W>
			<H>100</H>
		</BOUNDS>
		<ALIGN>
			<HALIGN>center</HALIGN>
			<VALIGN>bottom</VALIGN>
		</ALIGN>
		<VISIBLE>false</VISIBLE>
		<RESIZABLE>false</RESIZABLE>
		<FOCUSABLE>false</FOCUSABLE>
		<MOVABLE>false</MOVABLE>
	</FRAME>

	<!-- Slider : 리플레이 타임라인 슬라이더 -->
	<SLIDER item="ReplayTimeLine_Slider" parent="ReplayTimeLine_Frame">
		<THUMBLOOK>CustomTimelineThumbLook</THUMBLOOK>
		<BOUNDS>
			<X>0</X>
			<Y>35</Y>
			<W>240</W>
			<H>16</H>
		</BOUNDS>
		<ALIGN>
			<HALIGN>center</HALIGN>
			<VALIGN>top</VALIGN>
		</ALIGN>
	</SLIDER>

	<!-- Button : 리플레이 일시정지 버튼 -->
	<BMBUTTON item="ReplayPauseButton" parent="ReplayTimeLine_Frame">
	<BUTTONLOOK>NormalBmButtonLook</BUTTONLOOK>
		<BOUNDS>
			<X>0</X>
			<Y>-20</Y>
			<W>40</W>
			<H>40</H>
		</BOUNDS>
		<ALIGN>
			<HALIGN>center</HALIGN>
			<VALIGN>bottom</VALIGN>
		</ALIGN>
		<STRETCH/>
		<BITMAP type="up">pauseBtn_normal.png</BITMAP>
		<BITMAP type="over">pauseBtn_overlap.png</BITMAP>
		<BITMAP type="down">pauseBtn_pressed.png</BITMAP>
		<TOOLTIP>STR:UI_TOOLTIP_REPLAY_PAUSE</TOOLTIP>
		<TEXT/>
	</BMBUTTON>

	<!-- Button : 리플레이 재생 버튼 -->
	<BMBUTTON item="ReplayPlayButton" parent="ReplayTimeLine_Frame">
	<BUTTONLOOK>NormalBmButtonLook</BUTTONLOOK>
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>40</W>
			<H>40</H>
		</BOUNDS>
		<ALIGN>
			<HALIGN>center</HALIGN>
			<VALIGN>bottom</VALIGN>
		</ALIGN>
		<STRETCH/>
		<BITMAP type="up">playBtn_normal.png</BITMAP>
		<BITMAP type="over">playBtn_overlap.png</BITMAP>
		<BITMAP type="down">playBtn_pressed.png</BITMAP>
		<TOOLTIP>STR:UI_TOOLTIP_REPLAY_PLAY</TOOLTIP>
		<TEXT/>
	</BMBUTTON>

	<!-- TEXTAREA : 표시 -->
	<TEXTAREA item="ReplayCurTime" parent="ReplayTimeLine_Frame">
		<FONT>FONTa10</FONT>
		<TEXTCOLOR>
			<R>210</R>
			<G>210</G>
			<B>210</B>
		</TEXTCOLOR>
		<BOUNDS>
			<X>0</X>
			<Y>45</Y>
			<W>40</W>
			<H>30</H>
		</BOUNDS>
		<EDITABLE>false</EDITABLE>
	</TEXTAREA>

	<!-- TEXTAREA : 표시 -->
	<TEXTAREA item="ReplayTotalTime" parent="ReplayTimeLine_Frame">
		<FONT>FONTa10</FONT>
		<TEXTCOLOR>
			<R>210</R>
			<G>210</G>
			<B>210</B>
		</TEXTCOLOR>
		<BOUNDS>
			<X>0</X>
			<Y>45</Y>
			<W>40</W>
			<H>30</H>
		</BOUNDS>
		<ALIGN>
			<HALIGN>right</HALIGN>
			<VALIGN>top</VALIGN>
		</ALIGN>
		<EDITABLE>false</EDITABLE>
	</TEXTAREA>
</XML>
